{
  "name": "pixelopen/sulu-newsbundle",
  "type": "sulu-bundle",
  "description": "Manage news for Sulu",
  "keywords": [
    "sulu",
    "suluCms"
  ],
  "homepage": "https://github.com/Pixel-Open/sulu-newsbundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Pixel Open"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "sulu/sulu": "^2.4",
    "symfony/config": "^4.4 || ^5.0",
    "pixelopen/sulu-blockbundle": "^2.4 || ^2.5",
    "symfony/dependency-injection": "^4.4 || ^5.0",
    "symfony/framework-bundle": "^4.4 || ^5.0",
    "symfony/http-foundation": "^4.4 || ^5.0",
    "symfony/http-kernel": "^4.4 || ^5.0"
  },
  "require-dev": {
    "dantleech/phpcr-migrations-bundle": "^1.3",
    "jackalope/jackalope-doctrine-dbal": "^1.3.2",
    "phpstan/extension-installer": "^1.2",
    "phpstan/phpstan": "^1.9",
    "phpstan/phpstan-symfony": "^1.2",
    "symplify/easy-coding-standard": "^11.1"
  },
  "scripts": {
  },
  "autoload": {
    "psr-4": {
      "Pixel\\NewsBundle\\": "src"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": false,
      "phpstan/extension-installer": false
    }
  }
}
